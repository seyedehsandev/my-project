{
  "kind": "collectionType",
  "collectionName": "vehicles",
  "info": {
    "singularName": "vehicle",
    "pluralName": "vehicles",
    "displayName": "vehicle",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "model": {
      "type": "string",
      "required": true
    },
    "brand": {
      "type": "string",
      "required": true
    },
    "plate": {
      "type": "string",
      "required": true
    },
    "year": {
      "type": "integer",
      "required": true,
      "min": 1350,
      "max": 1410
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "ون",
        "سواری",
        "تریلی",
        "کامیونت",
        "موتور"
      ],
      "required": true,
      "default": "کامیونت"
    },
    "vehicleStatus": {
      "type": "enumeration",
      "enum": [
        "فعال",
        "غیر فعال",
        "در دست تعمیر",
        "اسقاط",
        "در سفر"
      ],
      "required": true,
      "default": "فعال"
    },
    "color": {
      "type": "string",
      "required": true
    },
    "expireInsurance": {
      "type": "date",
      "required": true
    },
    "technicalInspectionExpireDate": {
      "type": "date",
      "required": true
    }
  }
}
